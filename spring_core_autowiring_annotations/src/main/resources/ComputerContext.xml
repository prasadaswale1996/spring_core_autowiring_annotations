<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<context:annotation-config />
	<bean id="Std" class="spring_core_autowiring_annotations.Student">
		<property name="name" value="Akshay"></property>
		<property name="roll" value="0709"></property>

	</bean>
	
    <bean id="addr1" class= "spring_core_autowiring_annotations.Address" >
		<property name="city" value="Pune"></property>
		<property name="pin" value="411048"></property>


	</bean>
	<bean id="addr2" class="spring_core_autowiring_annotations.Address">
		<property name="city" value="Mumbai"></property>
		<property name="pin" value="400055"></property>
	</bean>
</beans>